{
    "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/main/src/NerdBank.GitVersioning/version.schema.json",
    "version": "1.2",
    "publicReleaseRefSpec": [
        "^refs/heads/master$",
        "^refs/heads/v\\d+(?:\\.\\d+)?$"
    ],
    "cloudBuild": {
        "buildNumber": {
            "enabled": true
        }
    },
    "release": {
        "tagName": "v{version}",
        "branchName": "v{version}",
        "versionIncrement": "minor"
    },
    "inherit": false
}